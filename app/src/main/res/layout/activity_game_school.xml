<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="beauisbarbie.scl.GameSchoolActivity">

    <ImageButton
        android:id="@+id/lv1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="70dp"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <ImageButton
        android:id="@+id/lv2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/lv1"
        android:layout_marginEnd="78dp"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <ImageButton
        android:id="@+id/lv3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/lv1"
        android:layout_below="@+id/lv1"
        android:layout_marginStart="6dp"
        android:layout_marginTop="134dp"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <ImageButton
        android:id="@+id/lv4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/lv2"
        android:layout_alignTop="@+id/lv3"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <ImageButton
        android:id="@+id/lv5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/lv3"
        android:layout_marginBottom="99dp"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <ImageButton
        android:id="@+id/lv6"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/lv4"
        android:layout_alignTop="@+id/lv5"
        android:background="#ffffff"
        app:srcCompat="@drawable/gschool" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/lv1"
        android:layout_alignStart="@+id/lv1"
        android:layout_below="@+id/lv1"
        android:layout_marginTop="6dp"
        android:textColor="#000000"
        android:text="Lavel 1"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignEnd="@+id/lv2"
        android:layout_alignStart="@+id/lv2"
        android:textAlignment="center"
        android:textColor="#000000"
        android:layout_marginTop="6dp"
        android:text="Level 2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/lv3"
        android:layout_alignStart="@+id/lv3"
        android:layout_below="@+id/lv3"
        android:textAlignment="center"
        android:textColor="#000000"
        android:layout_marginTop="6dp"
        android:text="Level 3" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/lv4"
        android:layout_alignStart="@+id/lv4"
        android:layout_below="@+id/lv4"
        android:textAlignment="center"
        android:textColor="#000000"
        android:layout_marginTop="6dp"
        android:text="Level 4" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView16"
        android:layout_alignBottom="@+id/textView16"
        android:layout_alignEnd="@+id/lv5"
        android:layout_alignStart="@+id/lv5"
        android:textAlignment="center"
        android:textColor="#000000"
        android:layout_marginTop="6dp"
        android:text="Level 5" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/lv6"
        android:layout_alignStart="@+id/lv6"
        android:layout_below="@+id/lv6"
        android:layout_marginTop="6dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:text="Level 6" />

</RelativeLayout>
